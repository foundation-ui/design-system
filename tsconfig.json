{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2022",
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noUncheckedIndexedAccess": true,
    "resolveJsonModule": true,
    "noEmit": true
  },
  "exclude": ["node_modules", "dist"]
}
